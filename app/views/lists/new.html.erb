<h1>New List</h1>
<%= form_with model: @list, local: true do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control" %>
  </div>
  <%= f.submit "Create List", class: "btn btn-primary" %>
<% end %>
<%= link_to "Back to All Lists", lists_path, class: "btn btn-secondary" %>
